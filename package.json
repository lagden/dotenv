{
  "name": "@tadashi/dotenv",
  "version": "2.0.0",
  "description": ".env reader",
  "repository": "lagden/dotenv",
  "main": "index.js",
  "files": [
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "report": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "xo",
    "test": "NODE_ENV=test ava"
  },
  "keywords": [
    "environment",
    "environment-variables",
    "reader",
    "dotenv",
    "sync"
  ],
  "author": "Thiago Lagden",
  "license": "MIT",
  "engines": {
    "node": ">=8.9.1"
  },
  "devDependencies": {
    "ava": "1.4.1",
    "xo": "0.24.0"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "semicolon": false,
    "rules": {
      "camelcase": [
        "error",
        {
          "properties": "never"
        }
      ],
      "capitalized-comments": 0
    }
  }
}
